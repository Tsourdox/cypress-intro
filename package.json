{
  "name": "cypress-intro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run generate",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "serve:test": "next dev -p 3100 --turbopack",
    "cy:open": "cypress open --e2e -b chrome",
    "cy:run": "cypress run --e2e",
    "test": "dotenv -e .env.test -- start-server-and-test serve:test http://localhost:3100 cy:open",
    "test:run": "dotenv -e .env.test -- start-server-and-test serve:test http://localhost:3100 cy:run",
    "generate": "prisma generate",
    "push": "prisma db push",
    "studio": "prisma studio",
    "seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "dotenv": "^17.2.1",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/wait-on": "^5.3.4",
    "cross-env": "^7",
    "cypress": "^15.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "mongodb-memory-server": "^10.2.0",
    "prisma": "^6.14.0",
    "start-server-and-test": "^2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  }
}
